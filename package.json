{
  "name": "ssc2sma",
  "description": "Step converter from .ssc to .sma; supports the following formats: StepMania 5 (Official), PIU Infinity, StepF2.",
  "author": "Aldo Fregoso <aldomx@gmail.com>",
  "homepage": "http://ssc2sma.amx.io",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "yarpm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "gh-pages": "^2.0.1",
    "typescript": "^3.3.3",
    "yarpm": "^0.2.1"
  }
}
